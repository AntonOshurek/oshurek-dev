{
	"tabWidth": 2,
	"trailingComma": "all",
	"singleQuote": true,
	"semi": true,
	"printWidth": 100,
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "auto",
	"overrides": [
		{
			"files": "*.scss",
			"options": {
				"parser": "scss"
			}
		}
	],
	"ignore": ["**/node_modules/**"],
	"htmlVoidElements": ["img", "meta", "img"]
}
